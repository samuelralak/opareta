{
  "name": "@opareta/common",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.js",
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@opareta/source": "./src/index.ts",
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    }
  },
  "nx": {
    "name": "common",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "libs/common/dist",
          "tsConfig": "libs/common/tsconfig.lib.json",
          "packageJson": "libs/common/package.json",
          "main": "libs/common/src/index.ts",
          "assets": [
            "libs/common/*.md"
          ]
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{projectRoot}"
        ],
        "options": {
          "jestConfig": "libs/common/jest.config.cjs",
          "passWithNoTests": true
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
