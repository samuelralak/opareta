{
  "name": "@opareta/argus-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@opareta/argus"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/argus-e2e/jest.config.cts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@opareta/argus:build",
          "@opareta/argus:serve"
        ]
      }
    }
  }
}
